    <script
    document.addEventListener('DOMContentLoaded', function() {
        // Gestion des animations de la timeline
        const timelineProjects = document.querySelectorAll('.timeline-project');
        const timelineDots = document.querySelectorAll('.timeline-dot');
        
        let currentFilter = 'all';
        let searchQuery = '';

        // Fonction pour mettre à jour les compteurs de filtres
        function updateFilterCounts() {
            const counts = {
                'all': 0,
                'web': 0,
                'application': 0,
                'design': 0
            };

            projectCards.forEach(card => {
                if (!card.style.display || card.style.display !== 'none') {
                    const category = card.getAttribute('data-category');
                    counts[category]++;
        // Gestion des animations de la timeline
        timelineProjects.forEach(project => {
            const projectTitle = project.querySelector('h3')?.textContent || 'Projet';

            // Suivi des clics sur les liens
            project.querySelectorAll('a').forEach(link => {
                link.addEventListener('click', function(e) {
                    const isGithub = this.href.includes('github');
                    gtag('event', isGithub ? 'project_github' : 'project_view', {
                        'event_category': 'Projects',
                        'event_label': projectTitle
                    });
                });
            });
        });

        // Animation des points de la timeline
        timelineDots?.forEach(dot => {
            dot.addEventListener('mouseover', function() {
                this.classList.add('scale-125');
            });
            dot.addEventListener('mouseout', function() {
                this.classList.remove('scale-125');
            });
        });

        // Mettre en place le système d'intersection observer pour les animations
        const options = {
            root: null,
            rootMargin: '0px',
            threshold: 0.1
        };

        const handleIntersect = (entries, observer) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.classList.add('slide-visible');
                    observer.unobserve(entry.target);
                }
            });
        };

        const observer = new IntersectionObserver(handleIntersect, options);
        document.querySelectorAll('.slide-in-left, .slide-in-right').forEach(el => {
            observer.observe(el);
        });
                    <div>
                        <h3 class="text-xl font-bold text-gray-800 mb-2">Fonctionnalités clés</h3>
                        // Gestion des événements de suivi pour la navigation
                        document.querySelectorAll('.timeline-project').forEach(project => {
                            const projectTitle = project.querySelector('h3')?.textContent || 'Projet';
                            
                            // Suivi des clics sur les liens
                            project.querySelectorAll('a').forEach(link => {
                                link.addEventListener('click', function(e) {
                                    const isGithub = this.href.includes('github');
                                    gtag('event', isGithub ? 'project_github' : 'project_view', {
                                        'event_category': 'Projects',
                                        'event_label': projectTitle
                                    });
                                });
                            });
                        });
                        
                        // Animation au défilement avec Intersection Observer
                        const observerOptions = {
                            root: null,
                            rootMargin: '0px',
                            threshold: 0.1
                        };

        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    const card = entry.target;
                    if (currentFilter === 'all' || card.getAttribute('data-category') === currentFilter) {
                        card.classList.add('visible');
                    }
                    observer.unobserve(card);
                }
            });
        }, observerOptions);

        projectCards.forEach(card => {
            observer.observe(card);
        });

        // Fonction helper pour les détails des projets
